{
  "name": "Back2Work",
  "description": "Extension to redirect you to a website when you access some blocked websites.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/main.html",
    "default_icon": "images/gopher.png"
  },
  "permissions": ["storage", "tabs", "webNavigation", "notifications"],
  "background": {
    "service_worker": "scripts/background.js"
  }
}
